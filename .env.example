# Port Configuration
# Backend server port
BACKEND_PORT=3001

# Client (frontend) port
CLIENT_PORT=3000

# CORS Configuration
# Allowed origin for CORS (use specific URL in production, e.g., http://localhost:3000)
CORS_ORIGIN=*

# Signal API Configuration
# URL for the Signal API server (if using Signal tracking)
SIGNAL_API_URL=http://localhost:8080

# backend url
BACKEND_URL=http://localhost:3001

# Node Environment
NODE_ENV=production
